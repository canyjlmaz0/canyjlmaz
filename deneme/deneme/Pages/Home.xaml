<UserControl x:Class="deneme.Pages.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        
            <StackPanel MinWidth="200">
           
            <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBox"/>
            <TextBox x:Name="textBox1" Height="23" TextWrapping="Wrap" Text="TextBox"/>
            <Button x:Name="button" Content="Button" Width="76" Margin="96,0"/>
        </StackPanel>
       
    </Grid>
</UserControl>
